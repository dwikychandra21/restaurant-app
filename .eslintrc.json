{
  "env": {
    "browser": true,
    "es2021": true,
    "jasmine": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off"
  },
  "plugins": ["jasmine", "prettier"]
}
